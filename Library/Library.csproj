<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <LangVersion>7.3</LangVersion>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Extension for Microsoft Dependency Injection (.net core) - named instances</Description>
        <PackageProjectUrl>https://github.com/MarcinWorkDev/extensions-microsoft-di</PackageProjectUrl>
        <RepositoryUrl>https://github.com/MarcinWorkDev/extensions-microsoft-di</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>MARCIN.WORK</Authors>
        <AssemblyName>MWork.Extensions.Microsoft.DependencyInjection</AssemblyName>
        <RootNamespace>MWork.Extensions.Microsoft.DependencyInjection</RootNamespace>
        <TargetFrameworks>netstandard2.0;netcoreapp2.1;netcoreapp2.2</TargetFrameworks>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
    </ItemGroup>

</Project>
